        "react-markdown": "^9.0.1",
        "react-quill": "^2.0.0",
        "react-redux": "^9.1.2",
        "react-router": "^6.23.1",
        "react-router-dom": "^6.23.1",
        "react-scroll": "^1.9.0",
        "react-spinners": "^0.13.8",
        "react-spinners-kit": "^1.9.1",
        "react-syntax-highlighter": "^15.5.0",
        "react-toastify": "^10.0.5",
        "react-transition-group": "^4.4.5",
