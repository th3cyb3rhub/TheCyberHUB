        "crypto-js": "^4.2.0",
        "date-fns": "^3.6.0",
        "dompurify": "^3.1.3",
        "dotenv": "^16.4.5",
        "fs-extra": "^11.2.0",
        "highlight.js": "^11.9.0",
        "html2canvas": "^1.4.1",
        "jspdf": "^2.5.1",
        "jwt-decode": "^4.0.0",
        "moment": "^2.30.1",
        "path": "^0.12.7",
        "path-to-regexp": "^6.2.2",
